using System;
namespace program{
public class Hello{
    public static void Main(){
          calculator obj = new calculator();
          Console.WriteLine("addition is "  + obj.addition(10,2));
          Console.WriteLine("subtraction is "  + obj.subtraction(1,2));
          Console.WriteLine("exponeniate "  + obj.exponeniate(2));
           Console.WriteLine("multiply "  + obj.multiply(1,2));
          Console.WriteLine("divide "  + obj.divide(1,2));
       
    }
    class calculator{
        public int a = 10;
        public int b = 2;
        
        public double addition (double a , double b){
            return a + b;
        }
         public double subtraction (double a , double b){
            return a - b;
        }
         public double multiply (double a , double b){
            return a * b;
        }
         public double divide (double a , double b){
            return a / b;
        }
         public double exponeniate (double a){
            return Math.Exp(a);
        }
    }
    
}
}
